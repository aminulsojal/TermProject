<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.loginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="465.0" prefWidth="560.0">
         <children>
            <Text fill="#217697" layoutX="90.0" layoutY="179.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Text>
            <Text fill="#da2bc0" layoutX="90.0" layoutY="229.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
               <font>
                  <Font name="System Bold Italic" size="16.0" />
               </font>
            </Text>
            <TextField fx:id="textField" layoutX="178.0" layoutY="161.0" />
            <PasswordField fx:id="password" layoutX="178.0" layoutY="211.0" />
            <Button layoutX="155.0" layoutY="301.0" mnemonicParsing="false" onAction="#resetOnAction" text="Reset" textFill="#2220b5">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="login" layoutX="295.0" layoutY="301.0" mnemonicParsing="false" onAction="#loginOnAction" text="Login" textFill="#481ebc">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="signUp" layoutX="209.0" layoutY="397.0" mnemonicParsing="false" onAction="#signUPOnAction" prefHeight="30.0" prefWidth="62.0" text="SIGN UP" textFill="#5712eb" />
            <Text fill="#1721cf" layoutX="178.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Online  Examination">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Text>
            <Text fill="#2418c9" layoutX="304.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Test Your Knowledge">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Text>
            <Label layoutX="90.0" layoutY="367.0" prefHeight="30.0" prefWidth="171.0" text="Don't have an account ?" textFill="#07a5e0">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font></Label>
         </children>
      </AnchorPane>
   </children>
</GridPane>
